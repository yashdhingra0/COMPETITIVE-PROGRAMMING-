
////////////////////////////////////////////////////////////using recursion/////////////////////////////////////////////////////////
int lcs(string x,string y ,int n,int m){
if(n==0||m==0){
return 0;}
if(x[n-1]==y[m-1]){
return 1+lcs(x,y,n-1,m-1);}
else{
return max(lcs(x,y,n-1,m),lcs(x,y,n,m-1);}
}



//////////////////////////////////////////////////////using dp////////////////////////////////////////////////////////////////////
int t[1001][1001];
int lcs(string x,string y ,int n,int m){
memset(t,-1,sizeof(t));
if(n==0||m==0){
return 0;}
if(t[m][n]!=-1){
return t[m][n];}
if(x[n-1]==y[m-1]){
return t[m][n]=1+lcs(x,y,n-1,m-1);}
else{
return t[m][n]=max(lcs(x,y,n-1,m),lcs(x,y,n,m-1);}
}
