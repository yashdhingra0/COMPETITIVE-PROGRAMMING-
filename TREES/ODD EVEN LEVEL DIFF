ODDEVEN(NODE* A)
IF(A==NULL)
RETURN 0;
QUEUE<NODE*>Q;
QUEUE.PUSH(ROOT);
INT LEVEL=0;
INT OSUM=0;
INT ESUM=0;
WHILE(!Q.EMPTY())
INT SIZE=Q.SIZE();
LEVEL+=1;

WHILE(SIZE!=0)
NODE*TEMP=Q.FRONT();
Q.POP();
IF(LEVEL%2==0)
ESUM+=TEMP->DATA;
ELSE
OSUM+=TEMP->DATA;
IF(TEMP->LEFT)
Q.PUSH(TEMP->LEFT);
IF(TEMP->RIGHT)
Q.PUSH(TEMP->RIGHT);
SIZE=SIZE-1;
}}
RETURN OSUM-ESUM;}







