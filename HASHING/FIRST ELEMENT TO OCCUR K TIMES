#include<bits/stdc++.h>
using namespace std;
int solve(int a[],int n,int k){
    unordered_map<int,int>hash;
    for(int i=0;i<n;i++){
        hash[a[i]]++;
    }
    for(int i=0;i<n;i++){
        if(hash[a[i]]==k)
            return a[i];}
       // break;
        
        
            return -1;
        
            }
int main() 
{ 
    int arr[] = {1, 7, 4, 3, 4, 8, 7}; 
    int n = sizeof(arr) / sizeof(arr[0]); 
    int k = 2; 
    cout<<solve(arr, n, k); 
    return 0; 
}  
